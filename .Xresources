! ~/.Xresources

!! URXVT APPEARANCE
URxvt.font:                 xft:SF Mono:size=11
! URxvt.boldFont:           xft:robotomono nerd font:size=11
! URxvt.italicFont:         xft:robotomono nerd font:size=11
! URxvt.boldItalicFont:     xft:robotomono nerd font:size=11

! URxvt.font:		xft:DejaVu Sans Mono:size=11
! URxvt.boldFont: 	xft:DejaVu Sans Mono:style=Bold
! URxvt.italicFont:	xft:DejaVu Sans Mono:style=Oblique
! URxvt.boldItalicFont:	xft:DejaVu Sans Mono:style=Bold Oblique

URxvt.letterSpace: -1
URxvt.scrollBar:            false
URxvt.saveLines:            65535
! URxvt.internalBorder:       30
URxvt.cursorBlink: 1
! URxvt.cursorUnderline: 1
Urxvt.clipboardAutocopy:    true
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
! URxvt.iso14755: truevim 
! URxvt.iso14755_52: false

